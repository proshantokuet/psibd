<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.openmrs.module.PSI.SHNDhisIndicatorDetails" table="dhis_indicator_details" lazy="false">
        <id column="indicator_id" name="indicatorId" type="int">
            <generator class="native"/>
        </id> 
	    <property name="postJson" column="post_Json" type="java.lang.String"/>	
	    <property name="response" column="response" type="java.lang.String"/>   
	    <property name="url" column="url" type="java.lang.String"/>		
	   	<property name="error" column="error" type="java.lang.String"/>
	   	<property name="indicatorType" column="indicator_type" type="java.lang.String"/>
	   	<property name="referenceId" column="reference_id" type="java.lang.String"/>	
	   	<property name="status" column="status" type="int"/>
	    <property name="dateCreated" column="date_created" type="java.util.Date" not-null="true"/>
		<property name="dateChanged" column="date_changed" type="java.util.Date" />		
    </class>
</hibernate-mapping>